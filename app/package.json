{
  "name": "chronofresh",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"yarn dev:client\" \"yarn dev:server\"",
    "dev:server": "tsx watch server.ts",
    "dev:client": "webpack-dev-server --open --liveReload --config webpack.dev.cjs",
    "build": "rimraf dist && tsc -p tsconfig.json && yarn build:client",
    "build:client": "webpack --config webpack.prod.cjs",
    "serve": "cross-env NODE_ENV=production node ./dist/server.js",
    "clean": "rimraf dist/"
  },
  "type": "module",
  "dependencies": {
    "@formatjs/intl-locale": "^3.1.1",
    "@formatjs/intl-localematcher": "^0.2.32",
