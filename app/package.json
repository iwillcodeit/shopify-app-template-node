    "@shopify/app-bridge-react": "^3.7.7",
    "@shopify/i18next-shopify": "0.2.0",
    "@shopify/polaris": "^10.49.1",
    "@shopify/shopify-app-express": "^2.1.3",
    "@shopify/shopify-app-session-storage-memory": "^2.0.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "i18next": "^22.4.11",
    "i18next-resources-to-backend": "^1.1.3",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-i18next": "^12.2.0",
    "react-query": "^3.34.19",
    "react-router-dom": "^6.3.0",
    "react-server-dom-webpack": "18.3.0-canary-fdc8c81e0-20230707",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@shopify/stylelint-polaris": "^12.0.0",
    "@swc/core": "^1.3.93",
